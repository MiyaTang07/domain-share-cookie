#!/usr/bin/env node
const { Command } = require('commander')

const program = new Command()
program
  .version(require('../package').version)
  .usage('<command> [options]')
  .command('set <url>', 'add domain information')
  .command('remove <-f,-t>', 'remove one of domain information')
  .command('views', 'list all domains')
  .command('create ext', 'generate a chrome extension')

  program.parse(process.argv)